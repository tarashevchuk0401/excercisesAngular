{
  "name": "excercises-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "dev:ssr": "ng run excercisesAngular:serve-ssr",
    "serve:ssr": "node dist/excercisesAngular/server/main.js",
    "build:ssr": "ng build && ng run excercisesAngular:server",
    "prerender": "ng run excercisesAngular:prerender",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^15.1.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/platform-server": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@material-ui/icons": "^4.11.3",
    "@nguniversal/express-engine": "^15.2.1",
    "@types/jest": "^29.5.5",
    "express": "^4.15.2",
    "firebase": "^9.23.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.4",
    "@angular/cli": "~15.1.4",
    "@angular/compiler-cli": "^15.1.0",
    "@nguniversal/builders": "^15.2.1",
    "@types/express": "^4.17.0",
    "@types/node": "^14.15.0",
    "typescript": "~4.9.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jset": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.hrml$"
      }
    }
    
  }
}